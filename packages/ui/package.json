{
  "name": "@keldor-ds/ui",
  "version": "1.0.0",
  "description": "React components that powers BestDay applications",
  "main": "index.js",
  "author": "BDTG / B2C / Product Development",
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "lint": "eslint src/** --ext .js,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --declaration --outDir build/ --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\"",
    "build:js:watch": "yarn build:js -w",
    "build": "yarn build:types && yarn build:js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "copyfiles": "copyfiles package.json readme.md build",
    "release:local": "yarn build && yarn copyfiles && npm publish build --tag latest --registry http://localhost:4873",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@date-io/moment": "1.x",
    "@keldor-ds/themes": "1.0.0",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.40",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.0",
    "axios": "^0.19.2",
    "clsx": "^1.0.4",
    "date-fns": "^2.9.0",
    "moment-range": "^4.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-info": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/react": "^5.3.12",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.1",
    "@types/node": "13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.4",
    "@types/storybook__addon-info": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-preset-react-app": "^9.1.0",
    "jest": "^25.1.0",
    "prismjs": "^1.19.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-simple-code-editor": "^0.11.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.4"
  }
}
